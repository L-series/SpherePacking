N:=24;
k:=5;
M:=ModularForms(Gamma1(N),k);
B:=Basis(M);
E:=EisensteinSubspace(M);
E1:=Basis(E);

d:=Dimension(E);
xmax:=3;

for x in [1..d] do
	for y in [1..N^3] do
		sum :=0;
		for i in [1..N] do
			sum := sum + 1/(i^(k-1));
		end for;
		
			
		num:= Coefficient(E1[x],y)/(y^(k-1)) - (ZetaFunction(k-1)-sum)*xmax*NumberOfDivisors(N^2)*(NumberOfDivisors(N)+1)/2;
		print(num);

	end for;

end for;		
		
